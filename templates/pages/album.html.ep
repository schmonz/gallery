% layout 'default';
%= include 'partials/nav'
% foreach my $subalbum (@$subalbums) {
	<div class="album_thumb">
		<a href="<%= $subalbum->{link} %>"><img src="<%= $subalbum->{thumb} %>" /></a>
		<p><%= $subalbum->{name} %></p>
	</div>
% }
% foreach my $image (@$images) {
	<div class="image_thumb">
		<a href="<%= $image->{link} %>"><img src="<%= $image->{thumb} %>" /></a>
	</div>
% }
